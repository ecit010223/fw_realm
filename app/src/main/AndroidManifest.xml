<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.huier.fw_realm">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.IntroExampleActivity"
            android:launchMode="singleTop"/>
        <activity
            android:name=".activity.DocumentActivity"
            android:launchMode="singleTop"/>
        <activity
            android:name=".activity.DemoActivity"
            android:launchMode="singleTop"/>
        <activity
            android:name=".activity.QueryActivity"
            android:launchMode="singleTop"/>
        <activity android:name=".activity.RealmActivity">
        </activity>
        <activity android:name=".activity.ThreadActivity">
        </activity>
        <activity android:name=".activity.JsonActivity">
        </activity>
        <activity android:name=".activity.NotificationActivity">
        </activity>
        <activity android:name=".activity.MigrationActivity">
        </activity>
        <activity android:name=".activity.ListViewActivity">
        </activity>
        <activity android:name=".activity.RecyclerViewActivity">
        </activity>
        <activity android:name=".activity.GridViewActivity">
        </activity>
    </application>

</manifest>